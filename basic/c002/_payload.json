r extensive custom CSS.",{"type":31,"tag":86,"props":87,"children":89},"h4",{"id":88},"step-1-add-the-html-for-a-simple-tailwind-navbar",[90],{"type":37,"value":91},"Step 1: Add the HTML for a Simple Tailwind Navbar",{"type":31,"tag":93,"props":94,"children":96},"pre",{"code":95},"\n\u003Ctemplate>\n  \u003Cnav class=\"bg-indigo-800 text-white\">\n    \u003Cdiv class=\"container mx-auto px-4 md:flex items-center gap-6\">\n      \u003C!-- Logo -->\n      \u003Cdiv class=\"flex items-center justify-between md:w-auto w-full\">\n        \u003Ca href=\"#\" class=\"flex items-center py-2 px-1 text-white flex-1\">\n          \u003Cspan class=\"font-bold\">Data Science\u003C/span>\n        \u003C/a>\n        \u003C!-- Mobile Menu Button -->\n        \u003Cdiv class=\"md:hidden flex items-center\">\n          \u003Cbutton @click=\"toggleMobileMenu\" class=\"mobile-menu-button\">\n            \u003Csvg\n              xmlns=\"http://www.w3.org/2000/svg\"\n              width=\"24\"\n              height=\"24\"\n              viewBox=\"0 0 24 24\"\n            >\n              \u003Ctitle>bars-3-bottom-left\u003C/title>\n              \u003Cg fill=\"none\">\n                \u003Cpath\n                  d=\"M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25H12\"\n                  stroke=\"currentColor\"\n                  stroke-width=\"1.5\"\n                  stroke-linecap=\"round\"\n                  stroke-linejoin=\"round\"\n                >\u003C/path>\n              \u003C/g>\n            \u003C/svg>\n          \u003C/button>\n        \u003C/div>\n      \u003C/div>\n      \u003C!-- Primary Navigation -->\n      \u003Cdiv\n        class=\"hidden md:flex md:flex-row flex-col items-center justify-start md:space-x-1 navigation-menu pb-3 md:pb-0\"\n      >\n        \u003Ca href=\"#\" class=\"py-2 px-3 block text-white\">Home\u003C/a>\n        \u003Ca href=\"#\" class=\"py-2 px-3 block text-white\">About\u003C/a>\n        \u003C!-- Dropdown Menu -->\n        \u003Cdiv class=\"relative\" v-for=\"(item, index) in menuItems\" :key=\"index\">\n          \u003Cbutton\n            @click=\"toggleDropdown(index)\"\n            class=\"dropdown-toggle py-2 px-3 hover:bg-indigo-700 flex items-center gap-2 rounded\"\n          >\n            \u003Cspan class=\"pointer-events-none\">{{ item.name }}\u003C/span>\n            \u003Csvg\n              class=\"w-3 h-3 pointer-events-none\"\n              xmlns=\"http://www.w3.org/2000/svg\"\n              width=\"24\"\n              height=\"24\"\n              viewBox=\"0 0 24 24\"\n            >\n              \u003Ctitle>chevron-down\u003C/title>\n              \u003Cg fill=\"none\">\n                \u003Cpath\n                  d=\"M19.5 8.25l-7.5 7.5-7.5-7.5\"\n                  stroke=\"currentColor\"\n                  stroke-width=\"1.5\"\n                  stroke-linecap=\"round\"\n                  stroke-linejoin=\"round\"\n                >\u003C/path>\n              \u003C/g>\n            \u003C/svg>\n          \u003C/button>\n          \u003Cdiv\n            v-if=\"item.open\"\n            class=\"dropdown-menu absolute bg-indigo-700 text-white rounded-b-lg pb-2 w-48\"\n          >\n            \u003Ca\n              v-for=\"(submenu, i) in item.submenu\"\n              :key=\"i\"\n              :href=\"submenu.link\"\n              class=\"block pl-24 pr-2 py-2 hover:bg-indigo-800 text-white\"\n              >{{ submenu.name }}\u003C/a\n            >\n          \u003C/div>\n        \u003C/div>\n\n        \u003Ca href=\"#\" class=\"py-2 px-3 block text-white\">Contact\u003C/a>\n      \u003C/div>\n    \u003C/div>\n  \u003C/nav>\n\u003C/template>\n\n\u003Cscript>\nexport default {\n  data() {\n    return {\n      menuItems: [\n        {\n          name: \"Services1\",\n          open: false,\n          submenu: [\n            { name: \"Page1-1\", link: \"#\" },\n            { name: \"Page1-2\", link: \"#\" },\n            { name: \"Page1-3\", link: \"#\" },\n          ],\n        },\n        {\n          name: \"Services2\",\n          open: false,\n          submenu: [\n            { name: \"Page2-1\", link: \"#\" },\n            { name: \"Page2-2\", link: \"#\" },\n            { name: \"Page2-3\", link: \"#\" },\n          ],\n        },\n        {\n          name: \"Services3\",\n          open: false,\n          submenu: [\n            { name: \"Page3-1\", link: \"#\" },\n            { name: \"Page3-2\", link: \"#\" },\n            { name: \"Page3-3\", link: \"#\" },\n          ],\n        },\n      ],\n    };\n  },\n  methods: {\n    toggleDropdown(index) {\n      this.menuItems[index].open = !this.menuItems[index].open;\n    },\n    toggleMobileMenu() {\n      const mobileMenu = document.querySelector(\".navigation-menu\");\n      mobileMenu.classList.toggle(\"hidden\");\n    },\n  },\n};\n\u003C/script>\n\n\n",[97],{"type":31,"tag":98,"props":99,"children":100},"code",{"__ignoreMap":7},[101],{"type":37,"value":95},{"type":31,"tag":86,"props":103,"children":105},{"id":104},"step-2-responsive-design-with-tailwind-css",[106],{"type":37,"value":107},"Step 2: Responsive Design with Tailwind CSS",{"type":31,"tag":86,"props":109,"children":111},{"id":110},"step-3-toggling-the-dropdown-menu",[112],{"type":37,"value":113},"Step 3: Toggling the Dropdown Menu",{"type":31,"tag":86,"props":115,"children":117},{"id":116},"step-4-toggling-the-tailwind-navbar-mobile-menu",[118],{"type":37,"value":119},"Step 4: Toggling the Tailwind Navbar mobile menu",{"type":31,"tag":86,"props":121,"children":123},{"id":122},"step-5-customizing-and-testing-your-tailwind-navbar",[124],{"type":37,"value":125},"Step 5: Customizing and Testing Your Tailwind Navbar",{"type":31,"tag":80,"props":127,"children":128},{},[129],{"type":31,"tag":130,"props":131,"children":134},"img",{"alt":132,"src":133},"c002-01.jpeg","/images/basic/c002-01.jpeg",[],{"title":7,"searchDepth":10,"depth":10,"links":136},[137],{"id":34,"depth":10,"text":8,"children":138},[139,141],{"id":41,"depth":140,"text":44},3,